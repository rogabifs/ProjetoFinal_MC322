{
  "version": "0.2.0",
  "configurations": [
      {
          "name": ".NET Core Launch (console)",
          "type": "coreclr",
          "request": "launch",
          "preLaunchTask": "build",
          "program": "${workspaceFolder}/ProjetoFinalMC322/bin/Debug/net8.0/ProjetoFinalMC322.dll",
          "args": [],
          "cwd": "${workspaceFolder}/ProjetoFinalMC322/ProjetoFinalMC322",
          "stopAtEntry": false,
          "console": "internalConsole",
          "internalConsoleOptions": "openOnSessionStart",
          "launchBrowser": {
              "enabled": false
          },
          "env": {
              "ASPNETCORE_ENVIRONMENT": "Development"
          },
          "sourceFileMap": {
              "/Views": "${workspaceFolder}/Views"
          }
      }
  ]
}
